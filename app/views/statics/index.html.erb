<main id="main" class="todo bg-dark">
  <div class="inner">
    <div class="container">
      <header class="main-header text-center bg-secondary text-light p-5 m-5">
        <h1>TO DO LIST</h1>
        <h3 class="mb-0">THIS IS YOUR LIST AND YOU CAN SHARE IT TO YOUR MAIT</h3>
      </header>
      <div class="d-flex justify-content-center">
        <% if !logged_in? %>
          <%= link_to "LET'S SIGN UP", new_user_path, class: "btn btn-primary btn-lg" %>
        <% end %>
      </div>
    </div>
  </div>
</main>